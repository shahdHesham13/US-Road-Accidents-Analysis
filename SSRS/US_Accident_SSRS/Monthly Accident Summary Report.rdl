          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Light_Condition">
          <DataField>Light_Condition</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Total_Accidents">
