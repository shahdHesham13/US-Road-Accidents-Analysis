    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>US_Accident</DataSourceName>
        <CommandText>SELECT 
    D.Month_Name,
    W.Weather_Condition,
    L.Light_Condition,
    COUNT(A.Accident_SK) AS Total_Accidents,
    SUM(A.Num_Casualties) AS Total_Casualties
FROM Fact_Accident A
JOIN DimDate D ON A.Date_FK = D.Date_ID
JOIN Dim_Weather W ON A.Weather_FK = W.Weather_ID
JOIN Dim_Light L ON A.Light_Condition_FK = L.Light_ID
GROUP BY D.Month_Name, W.Weather_Condition, L.Light_Condition
ORDER BY D.Month_Name;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Month_Name">
          <DataField>Month_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Weather_Condition">
