          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="Avg_Speed">
          <DataField>Avg_Speed</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="Total_Vehicles">
          <DataField>Total_Vehicles</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
