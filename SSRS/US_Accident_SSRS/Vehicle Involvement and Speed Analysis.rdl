      <rd:SecurityType>Integrated</rd:SecurityType>
      <rd:DataSourceID>e07276b2-4090-4bd5-9422-f300aec95f5c</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT 
    V.Vehicle_Type_Name,
    G.Age_Group_Range,
    COUNT(F.Accident_Vehicle_ID) AS Total_Accidents,
    CAST(AVG(F.Vehicle_Speed) AS DECIMAL(10,2)) AS Avg_Speed,
    SUM(F.Num_Vehicles_Involved) AS Total_Vehicles
FROM Fact_Accident_Vehicle F
JOIN Dim_Vehicle_Type V ON F.Vehicle_Type_FK = V.Vehicle_Type_ID
JOIN Dim_Driver_Age_Group G ON F.Driver_Age_Group_FK = G.Driver_Age_Group_ID
GROUP BY V.Vehicle_Type_Name, G.Age_Group_Range
ORDER BY Avg_Speed  DESC;</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
